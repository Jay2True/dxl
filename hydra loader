keyInputBox:GetPropertyChangedSignal("Text"):Connect(function()
	if #keyInputBox.Text > 0 then
		task.spawn(function()
			for i = 1, 3 do
				typingIndicator.Text = string.rep(".", i)
				task.wait(0.3)
			end
			typingIndicator.Text = ""
		end)
	end
end)

